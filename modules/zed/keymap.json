[
  {
    "context": "GitPanel",
    "use_key_equivalents": true,
    "bindings": {
      "ctrl-f": "git::Fetch",
      "shift-p": "git::Push",
      "ctrl-p": "git::Pull",
      "ctrl-shift-p": "git::ForcePush",
      "ctrl-d": "git::Diff",
      "ctrl-g backspace": "git::RestoreTrackedFiles",
      "ctrl-g shift-backspace": "git::TrashUntrackedFiles",
      "ctrl-a": "git::StageAll",
      "ctrl-shift-a": "git::UnstageAll"
    }
  },
  {
    "context": "FileFinder || ContextEditor",
    "bindings": {
      "ctrl-w": "menu::SelectNext",
      "ctrl-s": "menu::SelectPrevious"
    }
  }
]
